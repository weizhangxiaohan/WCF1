<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
    <system.serviceModel>
        <bindings>
            <ws2007HttpBinding>
                <binding name="mySecurutyBinding">
                    <!--<security>
                        <message clientCredentialType="UserName" />
                    </security>-->
                </binding>
            </ws2007HttpBinding>
        </bindings>
        <behaviors>
            <serviceBehaviors>
                <behavior name="GetMetadata">
                    <serviceMetadata httpGetEnabled="true" />
                    <!--<serviceCredentials>
                      <serviceCertificate findValue="www.fabrikam.com" x509FindType="FindBySubjectName" storeLocation="LocalMachine" storeName="My" />
                      <userNameAuthentication userNamePasswordValidationMode="Custom" customUserNamePasswordValidatorType="AService.Services.MyCustomValidator,AService" />
                    </serviceCredentials>-->
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <service behaviorConfiguration="GetMetadata" name="AService.Services.ProductService">
                <endpoint address="http://localhost:9876/ProductService" binding="ws2007HttpBinding"
                    bindingConfiguration="mySecurutyBinding" contract="AService.Interfaces.IProductService">
                    <!--<identity>
                        <dns value="www.fabrikam.com" />
                    </identity>-->
                </endpoint>
              <endpoint address="MEX" binding="mexHttpBinding" contract="IMetadataExchange">                
              </endpoint>
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:9876/" />
                    </baseAddresses>
                </host>
            </service>
        </services>
    </system.serviceModel>
</configuration>
